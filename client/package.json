{
  "name": "Hotelapp client",
  "private": true,
  "version": "2",
  "description": "Client SPA for the Hotelapp microservices",
  "scripts": {
    "types": "tsc --noEmit",
    "dev": "node server.js",
    "serve": "npx webpack-cli server",
    "build": "webpack --env production --progress --config webpack.config.js",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json}\"",
